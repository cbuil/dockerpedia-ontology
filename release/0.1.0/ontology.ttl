###  http://dockerpedia.inf.utfsm.cl/vocab#hasImage
:hasImage rdf:type owl:ObjectProperty ;
          rdfs:domain :DockerFile ;
          rdfs:range :SoftwareImage ;
          rdfs:comment "A docker file has software images" .


